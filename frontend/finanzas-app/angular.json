{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "finanzas-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/finanzas-app",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "baseHref": "/finanzas/",
            "deployUrl": "/finanzas/",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "favicon.ico",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "baseHref": "/finanzas/",
              "deployUrl": "/finanzas/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "baseHref": "/finanzas/",
              "deployUrl": "/finanzas/",
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "host": "0.0.0.0",
            "port": 4200,
            "publicHost": "ryzenpc.mooo.com",
            "allowedHosts": [
              "ryzenpc.mooo.com"
            ]
          },
          "configurations": {
            "production": {
              "buildTarget": "finanzas-app:build:production"
            },
            "development": {
              "buildTarget": "finanzas-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "favicon.ico",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}